// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Customer {
  id        String   @id @default(cuid())
  firstName String
  lastName  String
  email     String
  avatar    String
}

model Meal {
  id          String @id @default(cuid())
  name        String
  description String
  prince      Int
}
